{
  "name": "nestjsbase",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert",
    "migration:create": "typeorm migration:create -n PostRefactoring",
    "migration:generate": "typeorm migration:generate -n PostRefactoring"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/platform-socket.io": "^8.3.1",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^8.3.1",
    "@socket.io/redis-adapter": "^7.1.0",
    "@truongezgg/nest-teams-webhook": "^2.0.9",
    "ajv": "^8.1.0",
    "ajv-formats": "^2.0.2",
    "aws-sdk": "^2.1082.0",
    "bcrypt": "^5.0.1",
    "bullmq": "^1.80.2",
    "cache-manager": "^3.6.0",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^8.2.0",
    "flat": "^5.0.2",
    "helmet": "^4.5.0",
    "ioredis": "^4.28.5",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "md5": "^2.3.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^6.2.2",
    "multer": "^1.4.4",
    "mysql2": "^2.2.5",
    "nestjs-throttler-storage-redis": "^0.1.18",
    "nodemailer": "^6.7.5",
    "onesignal-node": "^3.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4",
    "sharp": "^0.30.1",
    "string-format": "^2.0.0",
    "typeorm": "^0.2.32",
    "uninstall": "0.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@types/bcrypt": "^3.0.1",
    "@types/cache-manager": "^3.4.2",
    "@types/cache-manager-ioredis": "^2.0.2",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.11",
    "@types/flat": "^5.0.1",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.179",
    "@types/module-alias": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.14.36",
    "@types/node-fetch": "^2.6.1",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/sharp": "^0.29.5",
    "@types/string-format": "^2.0.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "socket.io-client": "^4.4.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^\\$config": "<rootDir>/src/config",
      "^\\$decorators/(.*)$": "<rootDir>/src/decorators/$1",
      "^\\$helpers/(.*)$": "<rootDir>/src/helpers/$1",
      "^\\$types/(.*)$": "<rootDir>/src/types/$1",
      "^\\$database/(.*)$": "<rootDir>/src/database/$1",
      "^\\$shared/(.*)$": "<rootDir>/src/app/shared/$1",
      "^\\$admin/(.*)$": "<rootDir>/src/app/admin/$1",
      "^\\$client/(.*)$": "<rootDir>/src/app/client/$1",
      "^\\$app/(.*)$": "<rootDir>/src/app/$1",
      "^\\$core/(.*)$": "<rootDir>/src/core/$1",
      "@libs/common/(.*)": "<rootDir>/libs/common/src/$1",
      "@libs/common": "<rootDir>/libs/common/src",
      "@libs/message/(.*)": "<rootDir>/libs/message/src/$1",
      "@libs/message": "<rootDir>/libs/message/src"
    },
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ]
  },
  "_moduleAliases": {
    "$database": "dist/database",
    "$core": "dist/core",
    "$app": "dist/app",
    "$shared": "dist/app/shared",
    "$admin": "dist/app/admin",
    "$client": "dist/app/client",
    "$helpers": "dist/helpers",
    "$types": "dist/types",
    "$config": "dist/config"
  }
}
